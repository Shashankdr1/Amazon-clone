 const [email, setEmail] = useState(UserInfo.email);\n    const [password, setPassword] = useState('');\n    const [confirmPassword, setConfirmPassword] = useState('');\n    const [{ loadingUpdate }, dispatch] = useReducer(reducer, {\n        loadingUpdate: false\n    })\n\n    const SubmitHandler = async (e) => {\n        e.preventDefault();\n        try {\n            const { data } = await axios.put('/api/users/profile', {\n                name,\n                email,\n                password,\n            },\n                {\n                    headers: { authorization: `Bearer ${UserInfo.token}` }\n                }\n            );\n            dispatch({\n                type: 'UPDATE_SUCCESS'\n            })\n            ctxDispatch({\n                type: 'USER_SIGNIN', payload: data\n            })\n            localStorage.setItem('UserInfo', JSON.stringify(data));\n            toast.success('Profile updated successfully');\n        }\n        catch (err) {\n            dispatch({\n                type: 'FETCH_FAIL',\n            })\n            toast.error(getError(err)); // getError is a helper function\n        }\n    }\n    return (\n        <div className='container small-container'>\n            <Helmet>\n                <title>Profile</title>\n            </Helmet>\n            <h1 className='my-3'>Profile</h1>\n            <form onSubmit={SubmitHandler}>\n                <Form.Group className='mb-3' controlId='name'>\n                    <Form.Label>Name</Form.Label>\n                    <Form.Control\n                        value={name}\n                        onChange={(e) => setName(e.target.value)}\n                        required\n                    />\n                </Form.Group>\n                <Form.Group className='mb-3' controlId='email'>\n                    <Form.Label>Email</Form.Label>\n                    <Form.Control\n                        type='email'\n                        value={email}\n                        onChange={(e) => setEmail(e.target.value)}\n                        required\n                    />\n                </Form.Group>\n                <Form.Group className='mb-3' controlId='password'>\n                    <Form.Label>Password</Form.Label>\n                    <Form.Control\n                        type='password'\n                        onChange={(e) => setPassword(e.target.value)}\n                        required\n                    />\n                </Form.Group>\n                <Form.Group className='mb-3' controlId='confirmpassword'>\n                    <Form.Label>Confirm Password</Form.Label>\n                    <Form.Control\n                        type='password'\n                        onChange={(e) => setConfirmPassword(e.target.value)}\n                        required\n                    />\n                </Form.Group>\n                <div className='mb-3'>\n                    <Button type='submit'>Update</Button>\n                </div>\n            </form>\n        </div>\n    )\n}\n","C:\\Users\\Shashank D R\\Desktop\\project6th\\myapp\\src\\screens\\CartHistoryScreen.js",[],[],"C:\\Users\\Shashank D R\\Desktop\\project6th\\myapp\\src\\screens\\PaymentScreen.js",[],[],"C:\\Users\\Shashank D R\\Desktop\\project6th\\myapp\\src\\screens\\CartScreen.js",[],[],"C:\\Users\\Shashank D R\\Desktop\\project6th\\myapp\\src\\screens\\SearchScreen.js",[],[],"C:\\Users\\Shashank D R\\Desktop\\project6th\\myapp\\src\\screens\\ProductScreen.js",[],[],"C:\\Users\\Shashank D R\\Desktop\\project6th\\myapp\\src\\components\\ProtectedRoute.js",[],[],"C:\\Users\\Shashank D R\\Desktop\\project6th\\myapp\\src\\components\\SearchBox.js",[],[],"C:\\Users\\Shashank D R\\Desktop\\project6th\\myapp\\src\\components\\MessageBox.js",[],[],"C:\\Users\\Shashank D R\\Desktop\\project6th\\myapp\\src\\components\\LoadingBox.js",[],[],"C:\\Users\\Shashank D R\\Desktop\\project6th\\myapp\\src\\components\\CheckOutSteps.js",[],[],"C:\\Users\\Shashank D R\\Desk